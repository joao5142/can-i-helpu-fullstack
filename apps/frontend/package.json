    "vue-router": "^4.3.3",
    "@can-i-helpu-ds/vue": "1.0.0",
    "@can-i-helpu-ds/prettier-config": "1.0.0",
    "@can-i-helpu-ds/eslint-config": "1.0.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.8.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.5.0",
